/ {
    macros {
      meh: meh {
          compatible = "zmk,behavior-macro";
          #binding-cells = <0>;
          wait-ms = <0>;
          bindings = <&macro_press>
              , <&kp LALT>
              , <&kp LCTRL>
              , <&kp LSHFT>
              , <&macro_pause_for_release>
              , <&macro_release>
              , <&kp LALT>
              , <&kp LCTRL>
              , <&kp LSHFT>;
        };
    };
};
